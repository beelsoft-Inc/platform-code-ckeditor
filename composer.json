{
    "name": "laravel-admin-ext/code-editor",
    "description": "Code editor extension for laravel-admin based on code-mirror",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "code mirror", "code", "editor"],
    "license": "MIT",
    "authors": [
        {
            "name": "tungnt",
            "email": "tungnt.code@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tungnt/laravel-admin": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Tungnt\\Admin\\CodeEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tungnt\\Admin\\CodeEditor\\CodeEditorServiceProvider"
            ]
        }
    }
}
